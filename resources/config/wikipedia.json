{
  "corpus_id": "wikipedia",
  "corpus_name": {
    "eng": "Swedish Wikipedia Corpus",
    "swe": "Svenska Wikipedia-korpusen"
  },
  "corpus_description": {
    "eng": "Corpus of Swedish Wikipedia",
    "swe": "Svenska Wikipedia-korpusen"
  },
  "file_name_pattern": "*wiki*.xml",

  "analyze_config": {
    "word_attributes": [
      {
          "name": "wid",
          "set": false
      },
      {
          "nodeName": "lex",
          "name": "lemgram",
          "set": true
      }
    ],
    "struct_attributes": {
      "sentence": [
        {
          "name": "id",
          "set": false
        },
        {
          "name": "geocontext",
          "nodeName": "_geocontext",
          "set": true,
          "index_in_text": false,
          "properties": {
            "country": {
              "value": ";(.*);.*;",
              "type": "keyword"
            },
            "place": {
              "value": "(.*);.*;.*;",
              "type": "keyword"
            },
            "geopoint": {
              "type": "geopoint",
              "properties": {
                "lat": {
                  "value": ";.*;(.*);"
                },
                "lon": {
                  "value": ".*;(.*)"
                }
              }
            }
          }
        }
      ],
      "ne": [
        {
          "name": "ex",
          "set": false
        },
        {
          "name": "type",
          "set": false
        },
        {
          "name": "subtype",
          "set": false
        },
        {
          "name": "name",
          "set": false
        }
      ]
    },
    "text_attributes": [
      {
        "name": "title"
      },
      {
        "name": "url"
      }
    ]
  },
  "parent": "word-parent",
  "document_id": "title",
  "document_id_hash": true
}
